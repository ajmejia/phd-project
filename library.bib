@article{Rothberg2013,
abstract = {We present the first central velocity dispersions (sigma\_o) measured from the 0.85 micron Calcium II Triplet (CaT) for 8 advanced (i.e. single nuclei) local (z < 0.15) Ultraluminous Infrared Galaxies (ULIRGs). First, these measurements are used to test the prediction that the "sigma-Discrepancy," in which the CaT sigma\_o is systematically larger than the sigma\_o obtained from the 1.6 or 2.3 micron stellar CO band-heads, extends to ULIRG luminosities. Next, we combine the CaT data with rest-frame I-band photometry obtained from archival Hubble Space Telescope data and the Sloan Digital Sky Survey (SDSS) to derive dynamical properties for the 8 ULIRGs. These are then compared to the dynamical properties of 9,255 elliptical galaxies from the SDSS within the same redshift volume and of a relatively nearby (z < 0.4) sample of 53 QSO host galaxies. A comparison is also made between the I-band and H-band dynamical properties of the ULIRGs. We find four key results: 1) the sigma-Discrepancy extends to ULIRG luminosities; 2) at I-band ULIRGs lie on the Fundamental Plane (FP) in a region consistent with the most massive elliptical galaxies and not low-intermediate mass ellipticals as previously reported in the near-infrared; 3) the I-band M/L of ULIRGs are consistent with an old stellar population, while at H-band ULIRGs appear significantly younger and less massive; and 4) we derive an I-band Kormendy Relation from the SDSS ellipticals and demonstrate that ULIRGs and QSO host galaxies are dynamically similar.},
annote = {En este art\'{\i}culo tratan de ver si existe alg\'{u}n v\'{\i}nculo evolutivo entre las ULIRGs y las galaxias con n\'{u}cleos activos. Utilizan el plano fundamental para diagnosticar si las propiedades din\'{a}micas de las ULIRGs son similares a las de las galaxias con n\'{u}cleos activos.},
archivePrefix = {arXiv},
arxivId = {1302.1680},
author = {Rothberg, Barry and Fischer, Jacqueline and Rodrigues, Myriam and Sanders, D. B.},
eprint = {1302.1680},
file = {:home/alfredo/Library/Papers/Rothberg et al. - 2013.pdf:pdf},
month = feb,
title = {{Unveiling The Sigma-Discrepancy II: Revisiting the Evolution of ULIRGs \& The Origin of Quasars}},

year = {2013}
}
@article{Do2013,
abstract = {We present new high angular resolution near-infrared spectroscopic observations of the nuclear star cluster surrounding the Milky Way's central supermassive black hole. Using the integral-field spectrograph OSIRIS on Keck II behind the laser-guide-star adaptive optics system, this spectroscopic survey enables us to separate early-type (young, 4-6 Myr) and late-type (old, >1 Gyr) stars with a completeness of 50\% down to K' = 15.5 mag, which corresponds to \~{}10 $\backslash$msun for the early-type stars. This work increases the radial extent of reported OSIRIS/Keck measurements by more than a factor of 3 from 4" to 14" (0.16 pc to 0.56 pc), along the projected disk of young stars. For our analysis, we implement a new method of completeness correction using a combination of star-planting simulations and Bayesian inference. We assign probabilities for the spectral type of every source detected in deep imaging down to K' = 15.5 mag using information from spectra, simulations, number counts, and the distribution of stars. The inferred radial surface-density profiles, \$\backslash Sigma(R) \backslash propto R\^{}\{-\backslash Gamma\}\$, for the young stars and late-type giants are consistent with earlier results (\$\backslash Gamma\_\{early\} = 0.93 \backslash pm 0.09\$, \$\backslash Gamma\_\{late\} = 0.16 \backslash pm 0.07\$). The late-type surface-density profile is approximately flat out to the edge of the survey. While the late-type stellar luminosity function is consistent with the Galactic bulge, the completeness-corrected luminosity function of the early-type stars has significantly more young stars at faint magnitudes compared to previous surveys with similar depth. This luminosity function indicates that the corresponding mass function of the young stars is likely less top-heavy than that inferred from previous surveys.},
archivePrefix = {arXiv},
arxivId = {1301.0539},
author = {Do, Tuan and Lu, Jessica R and Ghez, Andrea M and Morris, Mark R and Yelda, Sylvana and Martinez, Gregory D and Wright, Shelley A and Matthews, Keith},
eprint = {1301.0539},
file = {:home/alfredo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded//Do et al. - 2013.pdf:pdf},
month = jan,
title = {{Stellar Populations in the Central 0.5 pc of the Galaxy I: A New Method for Constructing Luminosity Functions and Surface-Density Profiles}},

year = {2013}
}
