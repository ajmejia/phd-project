keywords = {classification,colours,etc,evolution,fundamental,galax-,galaxies,ies,luminosities,masses,photometry,radii,stellar content},
month = feb,
number = {February},
pages = {1--12},
title = {{Analytical approximations of K-corrections in optical and near-infrared bands}},

volume = {12},
